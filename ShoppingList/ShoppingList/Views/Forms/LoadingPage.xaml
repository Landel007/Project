             x:Class="ShoppingList.Views.Forms.LoadingPage"
             xmlns:busyindicator="clr-namespace:Syncfusion.SfBusyIndicator.XForms;assembly=Syncfusion.SfBusyIndicator.XForms">
    <ContentPage.Content>
        <StackLayout VerticalOptions="FillAndExpand" HorizontalOptions="CenterAndExpand">
            <busyindicator:SfBusyIndicator x:Name="busyindicator" Title="Betöltés..." TitlePlacement="Top" AnimationType="Cupertino" VerticalOptions="CenterAndExpand"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>